    "karma-coverage": "^0.4.0",
    "karma-jasmine": "^0.3.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.0",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.0.0",
    "less": "^2.0.0",
