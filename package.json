{
  "name": "llama",
  "version": "0.0.1",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "clean": "rimraf dist/* build/* coverage",
    "start": "cross-env NODE_ENV=production node ./webpack.server.js",
    "build": "npm run clean & cross-env NODE_ENV=production webpack --config webpack.build.js --bail -p",
    "build:offline": "npm run build -- --OFFLINE_MODE",
    "build:online": "npm run build -- --ONLINE_MODE",
    "dev": "cross-env NODE_ENV=development nodemon ./webpack.server.js",
    "dev:offline": "npm run dev -- --OFFLINE_MODE",
    "dev:online": "npm run dev -- --ONLINE_MODE",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "author": "xuan.0211@gmail.com",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.0.0",
    "angular-animate": "^1.5.8",
    "angular-bootstrap": "^0.12.0",
    "angular-resource": "^1.0.0",
    "angular-route": "^1.0.0",
    "angular-swipe": "^0.4.0",
    "angular-swiper": "^0.4.0",
    "angular-touch": "^1.5.8",
    "angular-translate": "^2.0.0",
    "bootstrap": "^3.3.0",
    "font-awesome": "^4.6.0",
    "jquery": "^3.1.0",
    "swiper": "^3.3.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.0.0",
    "autoprefixer-core": "^5.0.0",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-decorators": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "babel-preset-stage-1": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-preset-stage-3": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "copy-webpack-plugin": "^3.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.14.0",
    "cssnext-loader": "^1.0.0",
    "express": "^4.0.0",
